{%- comment %}<locksmith:36f9>{% endcomment -%}
{%- include 'locksmith' -%}
{%- comment %}</locksmith:36f9>{% endcomment -%}

<!DOCTYPE html>
<html lang="{{ shop.locale }}" class="no-js no-touch">
<head>
  {{ locksmith_initializations }}
  
  <!-- Meta Tags -->
  <meta charset="utf-8">
  <meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'>
  <meta name="viewport" content="width=device-width, height=device-height, initial-scale=1.0, minimum-scale=1.0">
  <meta name="google-site-verification" content="Gb_z5aPqvbSV539HNOydeQGYhV6VwQKrwEp74sn1Sjc" />
  <meta name="facebook-domain-verification" content="2q749o5bvcnf457tyerpu0ei4glb3k" />
  <link rel="canonical" href="{{ canonical_url }}" />
  
  <!-- Prefetch & Preload -->
  <link rel="prefetch" href="{{ shop.url }}" as="document">
  <link rel="preload" href="{{ 'theme.css' | asset_url }}" as="style">
  <link rel="preload" href="{{ 'theme.js' | asset_url }}" as="script">
  <link rel="preload" href="{{ 'lazysizes.min.js' | asset_url }}" as="script">
  <link rel="preload" href="{{ 'loading.svg' | asset_url }}" as="image">
  
  <!-- DNS Prefetch & Preconnect -->
  <link rel="dns-prefetch" href="https://cdn.shopify.com">
  <link rel="dns-prefetch" href="https://v.shopify.com">
  <link rel="dns-prefetch" href="https://fonts.googleapis.com" crossorigin>
  <link rel="dns-prefetch" href="https://cdn.shopifycloud.com">
  <link rel="dns-prefetch" href="https://cdnjs.cloudflare.com">
  <link rel="dns-prefetch" href="https://www.google-analytics.com">
  <link rel="preconnect" href="https://fonts.shopifycdn.com" crossorigin>
  <link rel="preconnect" href="https://ajax.googleapis.com">
  <link rel="preconnect" href="https://customjs.s.asaplabs.io">
  <link rel="dns-prefetch" href="https://monorail-edge.shopifysvc.com">
  
  <!-- Stylesheets -->
  {{- 'theme.dev.css' | asset_url | stylesheet_tag -}}
  
  <!-- Scripts -->
  <script defer src="{{ 'theme.js' | asset_url }}"></script>
  <script async src="{{ 'lazysizes.min.js' | asset_url }}"></script>
  <script async src="{{ 'custom-js.js' | asset_url }}"></script>
  
  <!-- Google Tag Manager -->
  <script>
    (function(w,d,s,l,i){
      w[l]=w[l]||[];w[l].push({'gtm.start': new Date().getTime(),event:'gtm.js'});
      var f=d.getElementsByTagName(s)[0], j=d.createElement(s), dl=l!='dataLayer'?'&l='+l:'';
      j.async=true; j.src='https://www.googletagmanager.com/gtm.js?id='+i+dl;
      f.parentNode.insertBefore(j,f);
    })(window,document,'script','dataLayer','GTM-PNNV4J');
  </script>
  
  <!-- Title and Description -->
  <title>{{ page_title }}{% if current_tags %} &ndash; {{ 'general.meta.tags' | t: tags: current_tags | join: ', ' }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}</title>
  {%- if page_description -%}
    {%- comment %}<locksmith:c997>{% endcomment -%}
      {%- capture var %}{% render 'locksmith-variables', variable: 'transparent', hiding_scope: 'resource' %}{% endcapture %}{% if var == 'true' %}<meta name="description" content="{{ page_description | escape }}">{% endif -%}
    {%- comment %}</locksmith:c997>{% endcomment -%}
  {%- endif -%}

  <!-- Icons & Favicon -->
  {%- if settings.favicon != blank -%}
    <link rel="shortcut icon" href="{{ settings.favicon | img_url: '32x32' }}" type="image/png" />
  {%- endif -%}

  <!-- External Scripts -->
  <script src="{{ '//ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js' | script_tag }}"></script>
  {{ 'daa.js' | asset_url | script_tag }}
  
  <!-- IE11 Polyfill -->
  <script>
    if (window.MSInputMethodContext && document.documentMode) {
      var scripts = document.getElementsByTagName('script')[0];
      var polyfill = document.createElement("script");
      polyfill.defer = true;
      polyfill.src = "{{ 'ie11.js' | asset_url }}";
      scripts.parentNode.insertBefore(polyfill, scripts);
      document.documentElement.classList.add('ie11');
    } else {
      document.documentElement.className = document.documentElement.className.replace('no-js', 'js');
    }
  </script>
  
  <!-- Theme & LazySizes Config -->
  <script>
    window.theme = {
      assets: {
        smoothscroll: '{{ "smoothscroll.js" | asset_url }}',
      },
      routes: {
        root_url: {{ routes.root_url | json }},
        cart_url: {{ routes.cart_url | json }},
        cart_add_url: {{ routes.cart_add_url | json }},
        cart_change_url: {{ routes.cart_change_url | json }},
        shop_url: {{ shop.url | json }},
        search_url: {{ routes.search_url | json }},
        product_recommendations_url: {{ routes.product_recommendations_url| json }},
      },
      strings: {
        loading: {{ "collections.pagination.loading" | t | json }},
        load_more: {{ "collections.pagination.load_more" | t | json }},
        add_to_cart: {{ "products.product.add_to_cart" | t | json }},
        remove: {{ 'cart.general.remove' | t | json }},
        sold_out: {{ "products.product.sold_out" | t | json }},
        sold_out_html: {{ "products.product.sold_out_html" | t | json }},
        on_sale_html: {{ "products.product.on_sale_html" | t | json }},
        unavailable: {{ "products.product.unavailable" | t | json }},
        zero_qty_error: {{ "cart.general.zero_qty_error" | t | json }},
        cart_title: {{ "cart.general.title" | t | json }},
        cart_empty: {{ "cart.general.empty" | t | json }},
        cart_price: {{ "cart.label.price" | t | json }},
        cart_quantity: {{ "cart.label.quantity" | t | json }},
        cart_total: {{ "cart.label.total" | t | json }},
        cart_acceptance_error: {{ "cart.general.accept_terms" | t | json }},
        limit_error: {{ "cart.general.limit_error" | t | json }},
        continue_shopping: {{ "cart.general.continue_shopping" | t | json }},
        results_one: {{ "general.search.results.one" | t | json }},
        results_other: {{ "general.search.results.other" | t | json }},
        no_results: {{ "general.search.results.no_results" | t | json }},
        view_all: {{ "general.search.view_all" | t | json }},
        unit_price_separator: {{ "general.accessibility.unit_price_separator" | t | json }},
      },
      icons: {
        plus: '{% render "icon-plus" %}',
        minus: '{% render "icon-minus" %}',
        close: '{% render "icon-close" %}',
      },
      settings: {
        cartType: {{ settings.cart_type | json }},
        gridImageSize: {{ settings.image_size | json }},
        enableInfinityScroll: {{ settings.enable_infinity_scroll | json }},
        enablePaymentButton: {{ settings.enable_payment_button | json }},
        enableReviews: {{ settings.enable_reviews | json }},
        showSaleBadge: {{ settings.show_sale_badge | json }},
        enableYotpo: {{ settings.enable_yotpo | json }},
        enableAcceptTerms: {{ settings.enable_accept_terms | json }},
        yotpoId: {{ settings.yotpo_ID | json }},
        productBadgeStyle: {{ settings.product_badge_style | json }},
        productGridSlideshow: {{ settings.product_grid_slideshow | json }},
        quickButton: {{ settings.quick_button | json }},
        mobileMenuBehaviour: {{ settings.mobile_menu_behaviour | json }},
      },
      moneyFormat: {{ shop.money_format | json }},
      info: { name: 'Palo Alto' },
      version: '3.0.1',
    };

    window.lazySizesConfig = window.lazySizesConfig || {};
    window.lazySizesConfig.preloadAfterLoad = true;
    window.PaloAlto = window.PaloAlto || {};
    window.slate = window.slate || {};
  </script>
  
  <!-- Styles for No-JS, Errors -->
  <style>
    html.no-js body { opacity: 0; transition: opacity 1s ease-in-out; }
    .page-width.page-width--standard { max-width: 1800px; }
  </style>

  <!-- External CSS -->
  {{ 'crossroad.css' | asset_url | stylesheet_tag }}
  
  <!-- Head closing -->
  {{ content_for_header }}
  {{ 'theme.css' | asset_url | stylesheet_tag }}
</head>

<body class="{{ template_name | replace: '.', '-' }} {{ settings.layout_class }} {{ settings.theme_class }}">
  {{ 'theme.js' | asset_url | script_tag }}
  <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PNNV4J" height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
  {{ content_for_body }}
  {{ content_for_footer }}
</body>
</html>
